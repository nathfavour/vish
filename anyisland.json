{
  "name": "vish",
  "version": "0.1.1",
  "description": "The Vibe Coders Shell - Security by Default, Instant Performance, AI-Integrated Intent Analysis",
  "repository": "github.com/nathfavour/vish",
  "toolchain": "go",
  "build": {
    "steps": [
      "mkdir -p bin",
      "CGO_ENABLED=0 go build -ldflags=-s -ldflags=-w -o bin/vish ./cmd/vish"
    ],
    "bin": "bin/vish"
  },
  "runtime": {
    "pulse": true,
    "managed_updates": true
  }
}
